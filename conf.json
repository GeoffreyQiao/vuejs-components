{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src/main.js", "src"],
        "exclude": ["node_modules"],
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["jsdoc-vue"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}